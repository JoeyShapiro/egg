// egg needs to supports conditionals
a = 69
b = 21

if a == 69 {
    print "nice"
}

// you can also do else
if a == b {
    print "yes"
} else {
    print "nope"
}

// however, egg does not have elif
// if you need that many, go to a check statement
which {
    a == b : { print "a == b" }
    //a >= b : { print "a >= b" }
    //b <= 0 : { print "b <= 0" }
    _ : { print "default case" }
}
